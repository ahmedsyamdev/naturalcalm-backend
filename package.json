{
  "name": "naturacalm-backend",
  "version": "1.0.0",
  "description": "Backend API for Naturacalm meditation app",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:clear": "ts-node src/scripts/seed.ts --clear",
    "seed:actual": "ts-node src/scripts/seedActualData.ts",
    "export-data": "ts-node src/scripts/exportData.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.921.0",
    "@aws-sdk/s3-request-presigner": "^3.921.0",
    "@types/mongoose": "^5.11.96",
    "@types/twilio": "^3.19.2",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "music-metadata": "^11.9.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "redis": "^5.9.0",
    "resend": "^6.7.0",
    "sharp": "^0.34.4",
    "stripe": "^19.2.0",
    "twilio": "^5.10.4",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
